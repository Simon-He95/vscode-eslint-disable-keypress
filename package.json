{
  "publisher": "simonhe",
  "name": "vscode-eslint-disable-keypress",
  "displayName": "vscode-eslint-disable-keypress",
  "version": "0.0.11",
  "packageManager": "pnpm@8.15.9",
  "description": "vscode-eslint-disable-keypress",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/vscode-eslint-disable-keypress#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/vscode-eslint-disable-keypress"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/vscode-eslint-disable-keypress/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-eslint-disable-keypress.on",
        "title": "快速生成 eslint-disable"
      }
    ],
    "keybindings": [
      {
        "command": "vscode-eslint-disable-keypress.on",
        "key": "cmd+shift+e",
        "when": "isMac"
      },
      {
        "command": "vscode-eslint-disable-keypress.on",
        "key": "ctrl+shift+e",
        "when": "isWin"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@types/node": "^18.19.121",
    "@types/vscode": "1.77.0",
    "@vscode-use/utils": "^0.1.59",
    "bumpp": "^9.11.1",
    "eslint": "^9.32.0",
    "lazy-js-utils": "^0.1.47",
    "pnpm": "^8.15.9",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vsce": "^2.15.0"
  }
}
